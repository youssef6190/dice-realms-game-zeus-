<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="896.0" prefWidth="1204.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.gui.MagentaBonusController">
   <children>
      <ImageView fx:id="BG" fitHeight="896.0" fitWidth="1204.0" onMouseEntered="#setLabel" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../resources/images/MagentaRealmBG.jpg" />
         </image>
      </ImageView>
      <StackPane layoutX="350.0" layoutY="198.0" prefHeight="150.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="PhoenixImageView" fitHeight="503.0" fitWidth="601.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/images/MagentaPhoneix.png" />
               </image>
            </ImageView>
            <Button fx:id="PhoenixButton" mnemonicParsing="false" onAction="#AttackPhoenix" opacity="0.0" prefHeight="500.0" prefWidth="532.0" text="Button" />
         </children>
      </StackPane>
      <Label fx:id="label" alignment="CENTER" layoutY="736.0" prefHeight="50.0" prefWidth="1204.0" textFill="WHITE">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
